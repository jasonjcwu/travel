// 性别computed
var transform = function (gender) {
  switch (gender) {
    case 0:
      return '../../../images/unman.png'
    case 1:
      return '../../../images/man.png'
    case 2:
      return '../../../images/woman.png'
  }
};

module.exports = {
  transform: transform
};
